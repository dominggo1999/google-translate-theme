{
  "name": "template",
  "version": "0.0.0",
  "watch": {
    "dev": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js,jsx,html"
    }
  },
  "scripts": {
    "devui": "vite",
    "watch": "npm-watch",
    "dev": "npm run clear && cross-env NODE_ENV=development && npm run prepare && npm run build",
    "build": "vite build",
    "serve": "vite preview",
    "clear": "rimraf extension/dist extension/manifest.json extension.*",
    "prepare": "node ./src/createManifest.js && node ./src/createThemeFile.js"
  },
  "dependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "cross-env": "^7.0.3",
    "npm-watch": "^0.11.0",
    "react": "^17.0.0",
    "react-colorful": "^5.5.1",
    "react-dom": "^17.0.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.0.2",
    "react-select": "^5.2.1",
    "react-tabs": "^3.2.3",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.0",
    "vite": "^2.6.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "tailwindcss": "^2.2.15",
    "twin.macro": "^2.7.0",
    "vite-plugin-babel-macros": "^1.0.5"
  }
}
